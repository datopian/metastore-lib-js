{
  "name": "metastore-lib-js",
  "version": "1.0.0",
  "description": "A JavaScript library for abstracting metadata storage for datapackage.json packages",
  "main": "dist/index.js",
  "scripts": {
    "test": "nyc mocha --timeout 100000 --require @babel/register test/*",
    "build": "rm -rf ./dist && babel ./src/ -d ./dist --no-comments",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "lint": "eslint ./src",
    "prepublishOnly": "npm run build",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datopian/metastore-lib-js.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/datopian/metastore-lib-js/issues"
  },
  "homepage": "https://github.com/datopian/metastore-lib-js#readme",
  "dependencies": {
    "@octokit/rest": "^18.0.12",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-modules-umd": "^7.10.4",
    "@babel/polyfill": "7.11.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.1",
    "babel-loader": "^8.1.0",
    "chai": "4.2.0",
    "core-js": "3.6.5",
    "coveralls": "3.1.0",
    "dotenv": "^8.2.0",
    "eslint": "7.10.0",
    "mocha": "8.1.3",
    "mocha-lcov-reporter": "1.3.0",
    "mocha-loader": "5.1.2",
    "nock": "13.0.4",
    "nyc": "15.1.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
